# Watson AI Configuration
WATSONX_API_KEY=your_watson_api_key_here
WATSONX_PROJECT_ID=your_watson_project_id_here
WATSONX_URL=https://us-south.ml.cloud.ibm.com
WATSONX_PLATFORM_URL=https://api.dataplatform.cloud.ibm.com
WATSONX_AUTHENTICATOR=iam

# Server Configuration
PORT=3000
NODE_ENV=production

# Database Configuration
MONGO_CONNECTION_URL=your_mongodb_connection_string_here

# JWT Configuration
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1d

# File Upload Configuration
MAX_FILE_SIZE=10mb
UPLOAD_PATH=./uploads

# Rate Limiting Configuration
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Debug Configuration
DEBUG=false
