{
  "version": 2,
  "builds": [
    {
      "src": "AnnDataAI/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "AnnDataAI/backend/api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/AnnDataAI/backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/AnnDataAI/frontend/dist/$1"
    }
  ]
}
